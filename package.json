  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@babel/parser": "^7.27.2",
    "@babel/traverse": "^7.27.1",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.3",
    "globby": "^14.1.0",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.7",
