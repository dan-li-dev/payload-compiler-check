  "author": "Daniel Li",
  "license": "ISC",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/payloadcms-compiler-check.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/payloadcms-compiler-check/issues"
